pipeline {
	agent any
	stages {
		stage('Build') {
			steps {
			     sh '''
				sleep 5
				echo "this is Build stage"
			      '''	
			}
		}
		stage('Test') {
			steps {
			    sh '''
				 sleep 5
				 echo "this is a Test stage"
			      '''
			}

		}
		stage ('Deploy') {
			steps {
			    sh '''
				sleep 5
				echo "this is a Deploy stage"
			      '''
			}
		}
		stage ('My-stage') {
			steps {
			   sh '''
				sleep 5
				echo "this is  a My stage"
			     '''
			}
		}
		
		}
}
